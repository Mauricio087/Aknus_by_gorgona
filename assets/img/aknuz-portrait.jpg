<svg width="400" height="400" viewBox="0 0 400 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Fondo místico -->
  <defs>
    <radialGradient id="mysticBg" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#4B0082;stop-opacity:0.8" />
      <stop offset="50%" style="stop-color:#8B4513;stop-opacity:0.6" />
      <stop offset="100%" style="stop-color:#2F1B69;stop-opacity:0.9" />
    </radialGradient>
    <filter id="glow">
      <feGaussianBlur stdDeviation="3" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/> 
      </feMerge>
    </filter>
  </defs>
  
  <!-- Fondo -->
  <rect width="400" height="400" fill="url(#mysticBg)"/>
  
  <!-- Estrellas de fondo -->
  <circle cx="50" cy="80" r="2" fill="#D4AF37" opacity="0.8"/>
  <circle cx="350" cy="120" r="1.5" fill="#D4AF37" opacity="0.6"/>
  <circle cx="80" cy="300" r="1" fill="#D4AF37" opacity="0.7"/>
  <circle cx="320" cy="350" r="2" fill="#D4AF37" opacity="0.5"/>
  <circle cx="150" cy="50" r="1" fill="#D4AF37" opacity="0.9"/>
  
  <!-- Silueta de persona -->
  <ellipse cx="200" cy="380" rx="80" ry="20" fill="#1a0f2e" opacity="0.3"/>
  
  <!-- Cuerpo -->
  <ellipse cx="200" cy="280" rx="60" ry="80" fill="#2F1B69" opacity="0.8"/>
  
  <!-- Cabeza -->
  <circle cx="200" cy="180" r="50" fill="#4B0082" opacity="0.9"/>
  
  <!-- Cabello místico -->
  <path d="M150 160 Q180 140 200 150 Q220 140 250 160 Q240 120 200 130 Q160 120 150 160" fill="#8B4513" opacity="0.7"/>
  
  <!-- Aura mística -->
  <circle cx="200" cy="180" r="70" fill="none" stroke="#D4AF37" stroke-width="2" opacity="0.4" filter="url(#glow)"/>
  <circle cx="200" cy="180" r="85" fill="none" stroke="#D4AF37" stroke-width="1" opacity="0.2" filter="url(#glow)"/>
  
  <!-- Cartas de tarot flotantes -->
  <g transform="translate(100,100) rotate(-15)">
    <rect x="0" y="0" width="30" height="45" rx="3" fill="#D4AF37" opacity="0.8"/>
    <rect x="2" y="2" width="26" height="41" rx="2" fill="#2F1B69"/>
    <circle cx="15" cy="22" r="8" fill="#D4AF37" opacity="0.6"/>
  </g>
  
  <g transform="translate(270,120) rotate(20)">
    <rect x="0" y="0" width="30" height="45" rx="3" fill="#D4AF37" opacity="0.7"/>
    <rect x="2" y="2" width="26" height="41" rx="2" fill="#2F1B69"/>
    <path d="M15 10 L20 20 L15 30 L10 20 Z" fill="#D4AF37" opacity="0.6"/>
  </g>
  
  <g transform="translate(120,250) rotate(-25)">
    <rect x="0" y="0" width="25" height="38" rx="2" fill="#D4AF37" opacity="0.6"/>
    <rect x="2" y="2" width="21" height="34" rx="1" fill="#2F1B69"/>
    <text x="12" y="22" text-anchor="middle" fill="#D4AF37" font-size="12" opacity="0.7">★</text>
  </g>
  
  <!-- Cristales místicos -->
  <polygon points="80,200 90,180 100,200 90,220" fill="#D4AF37" opacity="0.5" filter="url(#glow)"/>
  <polygon points="300,240 310,220 320,240 310,260" fill="#D4AF37" opacity="0.4" filter="url(#glow)"/>
  
  <!-- Símbolos místicos -->
  <g transform="translate(50,350)">
    <circle cx="0" cy="0" r="15" fill="none" stroke="#D4AF37" stroke-width="2" opacity="0.3"/>
    <path d="M-10,-10 L10,10 M10,-10 L-10,10" stroke="#D4AF37" stroke-width="2" opacity="0.3"/>
  </g>
  
  <g transform="translate(350,80)">
    <path d="M0,-12 L8,8 L-8,8 Z" fill="#D4AF37" opacity="0.4"/>
    <circle cx="0" cy="0" r="3" fill="#D4AF37" opacity="0.6"/>
  </g>
  
  <!-- Texto místico -->
  <text x="200" y="50" text-anchor="middle" fill="#D4AF37" font-family="serif" font-size="16" opacity="0.7" filter="url(#glow)">Aknuz</text>
  <text x="200" y="370" text-anchor="middle" fill="#D4AF37" font-family="serif" font-size="12" opacity="0.5">Tarotista Profesional</text>
</svg>